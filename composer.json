        "wpackagist-plugin/advanced-custom-fields": "^5.8",
        "wpackagist-plugin/hc-custom-wp-admin-url": "^1.4",
        "wpackagist-plugin/rus-to-lat-advanced": "^1.1",
        "wpackagist-plugin/wp-super-cache": "^1.7",
